		<LangVersion>latest</LangVersion>
		<Version>0.10.0</Version>
	</PropertyGroup>

	<PropertyGroup Label="Assembly information">
		<Authors>Leonardo Moreno</Authors>
		<Company>Codeit Corp</Company>
		<RepositoryUrl>https://github.com/Leitee/Codeit.Infrastructure.Identity</RepositoryUrl>
		<PackageTags>identity auth oidc oauth2</PackageTags>
		<Copyright>Copyright Â© Codeit 2019</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<OutputType>Library</OutputType>
